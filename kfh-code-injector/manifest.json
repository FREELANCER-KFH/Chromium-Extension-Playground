{
    "name": "kfh-code-injector",
    "version": "1.0.0",
    "description": "Injects your custom HTML, CSS and JavaScript to any website.",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "https://www.google.com/*",
                "https://www.microsoft.com/*"
            ],
            "js": [
                "app.js"
            ]
        }
    ]
}